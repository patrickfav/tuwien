        <component>
            <component-type>component.pain.HtmlImageLabelRadio</component-type>
            <renderer-type>component.pain.HtmlImageLabelRadioRenderer</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>spicsFileUpload</tag-name>
        <component>
            <component-type>component.fileupload.HtmlSpicsFileUpload</component-type>
           <!-- <renderer-type>component.fileupload.HtmlSpicsFileUploadRenderer</renderer-type> -->
