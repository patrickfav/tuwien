        <component>
            <component-type>component.vss.VancouverScarScale</component-type>
            <renderer-type>component.vss.VancouverScarScaleRenderer</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>colspan</tag-name>
        <component>
            <component-type>component.colspan.HTMLColSpan</component-type>
        </component>
    </tag>
    <tag>
