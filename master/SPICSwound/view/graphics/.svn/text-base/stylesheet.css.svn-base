/* IE Fix for png and hover effects 
img, div,input,td { behavior: url(./graphics/iefix.htc);  }
*/

body {								/* Main Page */
	margin-left:0px;
	margin-right:0px;
	margin-top:0px;
	margin-bottom:0px;
	background:url(./design/background.png) repeat-x #D7E1F1;
	font-family:Arial,sans-serif;
	font-size:15px;
}

input,textarea,select {				/* inputs for document (not main menu) */
	font-family:Arial,sans-serif;
	background:#fff;
	border:1px solid #074575;
	color:#000;
	font-size:14px;
	padding-left:2px;
	padding-right:2px;
}

form {
	margin:0px;
	padding:0px;
}

select {
	padding:0px;
}

select.fastswitch {							
	background:#fff;
	border:1px solid #ccc;
	padding:0px;
	font-size:12px;
}

input[disabled] {
	border:1px solid #aaa;
	color:#aaa;
}

select[disabled] {
	border:1px solid #aaa;
	color:#aaa;
}


tr.even {							/* even row of table */
	background:#fff;
}

tr.uneven {							/* uneven row of table */
	background:#E4E9F2;
}

a {									/* general document hyperlinks */
	color:#023F82;
	text-decoration:none;
}

a:hover {							/* hover of general document hyperlinks */
	color:#023F82;
	text-decoration:underline;
}

img {								/* images */
	border:0px;
}

h1 {
	margin-top:0px;
	margin-bottom:10px;
	font-family:Arial,sans-serif;
	font-size:15px;
	font-weight:bold;
}

h2 {
	margin-top:0px;
	margin-bottom:10px;
	font-family:Arial,sans-serif;
	font-size:12px;
	font-weight:bold;
}

p {
	margin-top:0px;
	margin-bottom:10px;
}

ul {
	padding:0px;
	margin:0px;
	padding-left:20px;
	padding-top:10px;
	padding-bottom:10px;
}

li {
	padding:0px;
	margin:0px;
}

#logo {
	padding-left:15px;
}

img.logoimg {
	behavior: url(./graphics/iefix.htc);
}

img.statusimg {
	behavior: url(./graphics/iefix.htc);
}

#top {								/* Header of Page */
	margin:0px;
	padding:0px;
}

.caption {
	font-weight:bold;
}

#header {							/* Header of Study-Document */	
	behavior: url(./graphics/iefix.htc);
	background-image: url(./design/studyheader_background.png);
	background-color:#ccd9ec;
	font-weight:bold;
	color:#074575;
}

#content {							/* main part of document (under main menu) */
	padding-top:40px;
	padding-bottom:40px;
	margin-left:20px;
	margin-right:20px;
}

#content td.left {					/* main left part of content */
	width:160px;
	padding-left:10px;
	padding-right:10px;
}

#content td.center {				/* main center part of content (like the real content) ;-) */
	padding:0px;
}

#content td.right {					/* main right part of content */
	width:160px;
	padding-left:10px;
	padding-right:10px;
}

#error_header {
	padding-top:20px;
	font-size:20px;
	color:#cc0000;
}
#error_nostyle {

}

#status {							/* Info-Box about Login-Status */
	font-size:12px;
	line-height:12px;
	margin-right:15px;
	border:1px #0a7636 solid;  /* B4C7D5  */
	padding-right:5px;
	padding-left:5px;
	padding-top:1px;
	padding-bottom:1px;
}

#status select {	/* font size of language select */
	font-size: 12px;	
}

#login td.header {
	behavior: url(./graphics/iefix.htc);
	background: url(./design/login.png) no-repeat;
	background-repeat:no-repeat;
	padding-left:15px;
	color:#fff;
	font-weight:bold;
	min-height:30px;
	height:26px;
	text-align:left;
}

input.button {
	background: #4477AA;
	color: white;
	margin: 5px;
	border-color: gray;
	behavior: url(./graphics/iefix.htc);
	width:120px;
	height:22px;
	font-weight:bold;
	font-size:14px;
}

